html{font-family:Georgia,times new roman,Times,serif}body{--palid-blue: #3D5873;font-family:Source Sans Pro}.bg-dark{background-color:#0e0e0e!important;color:#fff}section.first .jumbotron{background-color:#0e0e0e;color:#fff}.articles{padding:25px;background-color:var(--palid-blue)}.article{display:grid;grid-template-columns:50% 1em 50%;color:#fff;margin-top:25px}.article-img{max-width:100%;grid-column-start:1;grid-column-end:2}.article-title{font-size:18px}.article-text{grid-column-start:3;font-size:12px}.article-date{margin-bottom:0;font-size:12px}.articles button{grid-column-start:1;border-radius:5px;position:relative;border:0;width:50%;height:3rem;left:25%;margin-top:25px;margin-bottom:10px;font-family:Source Sans Pro;text-transform:uppercase;font-weight:700;font-size:18px;line-height:20px;color:#091440}h1{text-align:center;font-family:Source Sans Pro;text-transform:uppercase;font-weight:700}section.second{background-color:#4169e1;padding-top:25px;padding-bottom:25px}.jumbotron img{max-width:100%}#featured-jumbo{background-color:#2970ff!important}div#featured-jumbo.jumbotron.bg-dark div.row div.col-sm a.btn.btn-primary.btn-lg{background-color:#fff;color:#0e0e0e}#featured-img{object-fit:none;object-position:center;width:100%;max-height:300px}.content{color:#fff}.youtube{background:linear-gradient(140deg,#091440 0%,#091440 40%,#2970ff calc(40% + 1px),#2970ff 100%);color:#fff;padding:25px;text-align:center}.youtube img.youtube-logo{max-width:100%}.youtube iframe{width:100%;margin-bottom:10px;height:auto}.youtube-title{margin-top:px;text-transform:uppercase;font-weight:700}.youtube-desc{font-size:18px;text-transform:none;font-weight:400}section.separator-bar{background-color:#2ed6a1;padding:5px;display:grid}section.separator-bar-grid{background-color:#2ed6a1;padding:5px;display:grid;grid-template-columns:25px repeat(3,minmax(0,1fr)) 25px}.separator-bar-grid img{max-width:100%;grid-column-start:2;grid-column-end:3}.separator-bar-grid .text{grid-column-start:3;grid-column-end:5;margin:0;padding:5px}.separator-bar-grid .text button{background-color:#ffc84d;border-radius:25px;border:0;height:2em;padding:0 10px;margin-top:5px;font-family:Source Sans Pro;text-transform:uppercase;font-weight:700;font-size:12px;color:#091440}.separator-bar-grid .text p{text-transform:uppercase;font-size:16px;line-height:16px;text-align:left;margin:0}#unete{color:#091440;text-align:center;text-transform:uppercase;font-size:16px;font-weight:600}ul.social-media-list{list-style:none;text-align:center;padding:0;margin-top:0}.social-media-list li{display:inline}.social-media-list li a{vertical-align:middle;color:#091440;font-size:2em;text-align:center;margin:0 .25em}.page-footer{color:#fff}body{background-color:#0e0e0e}h1{color:#fff}