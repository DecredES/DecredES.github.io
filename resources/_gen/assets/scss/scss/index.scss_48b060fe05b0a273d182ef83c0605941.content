ete{font-size:1.2em}}.separator-bar .container .social-media-list{display:flex;list-style:none;margin:0}.separator-bar .container .social-media-list li{display:flex;margin-right:3rem}@media(max-width:576px){.separator-bar .container .social-media-list li{margin-right:1.2rem}}.separator-bar .container .social-media-list li a{color:#091440;font-size:3.2em;margin:0;text-decoration:none}.separator-bar .container .social-media-list li a:hover{opacity:.8}@media(max-width:475px){.separator-bar .container .social-media-list li a{font-size:1.5rem}}section.separator-bar-grid{background-color:#2ed6a1;border-top:5px inset rgba(0,0,0,.1);box-shadow:10px 0 0 rgba(0,0,0,.8);padding:.8rem 0}section.separator-bar-grid .container{align-items:center;display:flex;justify-content:center}section.separator-bar-grid .container img{margin-right:2rem;width:75px}section.separator-bar-grid .container .text{align-items:center;display:flex;margin:0;padding:5px}section.separator-bar-grid .container .text p{font-size:1.6em;font-weight:600;margin:0;margin-right:2rem;text-align:left;text-transform:uppercase}.separator-bar .container{display:flex;justify-content:center}.separator-bar .container .images-wrapper{margin-right:2rem}.separator-bar .container .images-wrapper img{width:57px}.separator-bar .container .text{display:flex}.separator-bar .container .text p{font-size:1.6em;font-weight:600;margin:0;margin-right:2rem;text-align:left;text-transform:uppercase}section.second{background-color:#4169e1;padding-top:25px;padding-bottom:25px}.jumbotron img{max-width:100%}#featured-jumbo{background-color:#2970ff!important}div#featured-jumbo.jumbotron.bg-dark div.row div.col-sm a.btn.btn-primary.btn-lg{background-color:#fff;color:#0e0e0e}#featured-img{object-fit:none;object-position:center;width:100%;max-height:300px}.content{color:#fff}.youtube{background:linear-gradient(140deg,#091440 0%,#091440 40%,#2970ff calc(40% + 1px),#2970ff 100%);color:#fff;padding:25px;text-align:center}.youtube img.youtube-logo{max-width:100%}.youtube iframe{width:100%;margin-bottom:10px;height:auto}.youtube-title{margin-top:px;text-transform:uppercase;font-weight:700}.youtube-desc{font-size:18px;text-transform:none;font-weight:400}.page-footer{color:#fff}h1{color:#fff}